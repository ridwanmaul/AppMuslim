<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="paket.example.ProjectUas">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=" Project UAS"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="paket.example.ProjectUas.MainMenuActivity"></activity>
        <activity android:name="paket.example.ProjectUas.DzikirSoreActivity" />
        <activity android:name="paket.example.ProjectUas.DzikirPagiActivity" />
        <activity android:name="paket.example.ProjectUas.Halal" />
        <activity android:name="paket.example.ProjectUas.Sholat" />
        <activity android:name="paket.example.ProjectUas.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>